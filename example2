person_1_age = 35  
person_2_age = 5

user_age = int(input("How old are you? "))

total_age = person_1_age + person_2_age + user_age

print(f"Person 1 is {person_1_age} years old and Person 2 is {person_2_age} years old. That means combined we are {total_age} years old.")

